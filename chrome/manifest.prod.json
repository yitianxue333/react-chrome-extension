{
  "version": "0.0.0",
  "name": "Timer",
  "manifest_version": 2,
  "description": "timer",
  "browser_action": {
    "default_title": "Timer"
  },
  "icons": {
    "16": "img/icon-16.png",
    "48": "img/icon-48.png",
    "128": "img/icon-128.png"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": ["tabs", "storage", "https://github.com/*" ],
  "content_security_policy": "default-src 'self'; script-src 'self'; style-src * 'unsafe-inline'; img-src 'self' data:;"
}
